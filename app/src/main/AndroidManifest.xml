<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ryankelly.ryankellyhospitalcost">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HospitalActivity" />
        <activity android:name=".Procedure" />
        <activity
            android:name=".PriceByHospital"
            android:label="@string/title_activity_price_by_hospital"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PriceByProcedure"
            android:label="@string/title_activity_price_by_procedure"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>